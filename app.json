{
  "expo": {
    "name": "Daily Rosary",
    "slug": "dailyrosary",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icons/ios-light.png",
    "scheme": "dailyrosary",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon":"./src/assets/icons/ios-light.png",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./src/assets/icons/adaptive-icon.png",
        "monochromeImage": "./src/assets/icons/adaptive-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.galleguillos.dailyrosary"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/icons/icons/splash-icon-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000",
            "image": "./src/assets/icons/icons/splash-icon-light.png"
          }
        }
      ],
      "expo-web-browser",
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0578cc9c-1315-464b-b67f-3064b3364ce1"
      }
    }
  }
}
